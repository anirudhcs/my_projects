
program pr3c37:
#
#	the output should be:
#	      1
#	      1
#	      1
#	
const k = 5;
      l = k;
      m = l;
      bool = true;
      c = 'a';
var i: integer;
    done : boolean;
    letter: char;
begin
   i := 4;
   case i of
	2..m: begin
		output(1);output(11111);
	      end;
        7..10: output(0);
   end;
   case bool of
     true: begin
		output(1);output(21111);
	      end;
     false: output(0);
   end;
   letter := 'b';
   case letter of
     c..'b': begin
		output(1);output(31111);
	      end;
     'c'..'d': output(0);
   end;
end pr3c37.
